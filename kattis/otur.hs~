check13:: Int->Int
check13 a=a!!xs
  where xs=[x|x<-[1..], not13 x]

not13:: Int->String
not13 x|x==read ("13")          =False
       |x==read ([a]++"13")     =False
       |x==read (as++"13")      =False
       |x==read ([a]++"13"++[a])=False
       |x==read ([a]++"13"++as) =False
       |x==read (as++"13"++[a]) =False
       |x==read (as++"13"++as)  =False
       |x==read ("13"++[a])     =False
       |x==read ("13"++as)      =False
       |otherwise               =True
